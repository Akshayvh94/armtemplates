{
  "location": "centralus",
  "name": "$AcsName$",
  "properties": {
    "orchestratorProfile": {
      "orchestratorType": "DCOS"
    },
    "masterProfile": {
      "count": 1,
      "dnsPrefix": "$AcsName$-$ResourceGroupName$-$Rand$m",
      "fqdn": "$AcsName$-$ResourceGroupName$-$Rand$.centralus.cloudapp.azure.com"
    },
    "agentPoolProfiles": [
      {
        "name": "$AcsName$pool",
        "count": 1,
        "vmSize": "Standard_D2_v2",
        "dnsPrefix": "$AcsName$-$ResourceGroupName$-$Rand$a",
        "fqdn": "$AcsName$-$ResourceGroupName$-$Rand$.centralus.cloudapp.azure.com"
      }
    ],
    "linuxProfile": {
      "ssh": {
        "publicKeys": [
          {
            "keyData": "ssh-rsa  rsa-key-20180525 user@hostname.local\n"
          }
        ]
      },
      "adminUsername": "azureuser"
    }
  }
}